{
  "name": "pencil",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "14.17.4"
  },
  "scripts": {
    "start": "NODE_ENV=development ts-node src/server.ts",
    "serve": "node build/server.js",
    "build": "yarn run clean && yarn run build-ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-node": "nodemon -r dotenv/config build/server.js",
    "clean": "rimraf ./build",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "eslint": "eslint . --ext .js,.ts",
    "oldTest": "jest --forceExit --detectOpenHandles --coverage --verbose",
    "test": "NODE_ENV=testing jest  --forceExit --runInBand --verbose src/services/**/*.test.ts"
  },
  "dependencies": {
    "@andreasremdt/simple-translator": "^2.0.4",
    "@google-cloud/storage": "^5.17.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "aws-sdk": "^2.1055.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.1",
    "boom": "^7.3.0",
    "bull": "^4.2.0",
    "cheerio": "^1.0.0-rc.10",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "jest": "^27.4.7",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "unfurl.js": "^5.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/boom": "^7.3.1",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7"
  }
}
